{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index-vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/index-vercel.js"
    },
    {
      "src": "/flutter/(.*)",
      "dest": "src/api/index-vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/api/index-vercel.js"
    }
  ]
}